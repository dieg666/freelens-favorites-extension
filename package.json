{
  "name": "freelens-favorites-extension",
  "version": "1.0.0",
  "description": "FreeLens extension that adds a favorites/bookmarking system for sidebar navigation items with drag-and-drop reordering",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delgadodiaz/freelens-favorites-extension.git",
    "directory": "."
  },
  "main": "out/main/index.js",
  "renderer": "out/renderer/index.js",
  "files": [
    "out/**/*"
  ],
  "engines": {
    "node": ">= 22.16.0",
    "freelens": "^1.6.0"
  },
  "license": "MIT",
  "author": {
    "name": "delgadodiaz",
    "email": "delgadodiaz@users.noreply.github.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "type:check": "tsc --noEmit -p tsconfig.json --composite false",
    "prebuild": "echo 'Skipping type check for now'",
    "build": "electron-vite build",
    "build:production": "VITE_PRESERVE_MODULES=false electron-vite build",
    "clean": "rm -rf out",
    "clean:all": "rm -rf out node_modules",
    "pack:dev": "pnpm build && pnpm pack",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@freelensapp/extensions": "^1.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "~22.15.35",
    "@types/react": "^17.0.90",
    "@vitejs/plugin-react": "^5.1.1",
    "electron-vite": "^4.0.1",
    "jest": "^29.7.0",
    "mobx": "6.13.7",
    "mobx-react": "7.6.0",
    "react": "17.0.2",
    "sass": "^1.94.1",
    "ts-jest": "^29.2.5",
    "typescript": "5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-external": "^6.2.2",
    "vite-plugin-sass-dts": "^1.3.34"
  },
  "keywords": [
    "freelens",
    "extension",
    "favorites",
    "bookmarks",
    "kubernetes",
    "navigation",
    "sidebar"
  ],
  "packageManager": "pnpm@10.22.0"
}
